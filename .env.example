# GenSelfie Configuration
# Copy this to .env and fill in your values

# Admin password (auto-generated on first run if not set)
ADMIN_PASSWORD=

# ComfyUI server URL
COMFYUI_URL=http://localhost:8188

# Stripe (optional)
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=

# LNbits (optional)
LNBITS_URL=
LNBITS_API_KEY=

# Debug mode
DEBUG=false

# Data storage directory (for RunPod, use /workspace)
# Database will be stored at DATA_DIR/genselfie.db
# Uploads will be stored at DATA_DIR/uploads/
# DATA_DIR=/workspace
